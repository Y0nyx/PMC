{
  "name": "dofa",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": {
    "name": "Dofa",
    "email": "dofaudes@skiff.com"
  },
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.3.1",
    "json2csv": "^6.0.0-alpha.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "react-scripts": "5.0.1",
    "react-simple-keyboard": "^3.7.151",
    "web-vitals": "^2.1.4",
    "ws": "^8.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "npm run build && electron .",
    "dist": "npm run build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^25.2.0",
    "electron-builder": "^24.9.1",
    "tailwindcss": "^3.3.2"
  },
  "build": {
    "asar": true,
    "extends": null,
    "appId": "com.dofa.jpl;",
    "linux": {
      "target": "deb",
      "category": "Utility",
      "icon": "build/logo512.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "configElectron.js",
      "configReact.js",
      "index.js",
      "apiAi.js",
      "apiFrontend.js",
      "apiPLC.js",
      "utils.js",
      "queries/**/*",
      "package.json",
      "protocol.json"
    ],
    "extraFiles": [
      {
        "from": "configElectron.js",
        "to": "./resources/configElectron.js"
      },
      {
        "from": "configReact.js",
        "to": "./resources/configReact.js"
      },
      {
        "from": "../sql/generateDatabase.sql",
        "to": "./resources/generateDatabase.sql"
      },
      {
        "from": "../docker-compose.yml",
        "to": "./resources/docker-compose.yml"
      },
      {
        "from": "../Dockerfile.pipelines",
        "to": "./resources/Dockerfile.pipelines"
      },
      {
        "from": "../Dockerfile.supervised",
        "to": "./resources/Dockerfile.supervised"
      },
      {
        "from": "../Dockerfile.unsupervised",
        "to": "./resources/Dockerfile.unsupervised"
      },
      {
        "from": "../Ai",
        "to": "./resources/Ai"
      },
      {
        "from": "plc.py",
        "to": "./resources/plc.py"
      }
    ]
  }
}
